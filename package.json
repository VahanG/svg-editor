    "react-jss": "^10.9.2",
    "sass": "^1.70.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@commitlint/cz-commitlint": "^16.3.0",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "commitizen": "^4.2.6",
    "compare-versions": "^4.1.4",
    "danger": "^11.2.1",
    "env-cmd": "^10.1.0",
    "lint-staged": "^12.5.0",
    "micromatch": "^4.0.5",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.4"
  },
  "browserslist": {
