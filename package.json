{
  "name": "svg-editor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "danger": "danger ci",
    "start": "react-scripts start",
    "start1": "PORT=3001 react-scripts start",
    "eject": "react-scripts eject",
    "commit": "git cz --hook",
    "check:ts": "tsc --noEmit",
    "check:lint": "eslint 'src/**/*.{js,ts,tsx,jsx}' --quiet",
    "build": "env-cmd -f .env react-scripts build && cd build && zip -r build.zip *",
    "build:prod": "env-cmd -f .env.production react-scripts build && cd build && zip -r build.zip *",
    "check:lint-fix": "eslint 'src/**/*.{js,ts,tsx,jsx}' --quiet --fix",
    "check:lint-ci": "eslint 'src/**/*.{js,ts,tsx,jsx}' -f json -o eslint-report.json"
  },
  "dependencies": {
    "paper": "^0.12.17",
    "react": "^18.2.0",
